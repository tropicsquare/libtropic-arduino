  "repository": {
    "type": "git",
    "url": "https://github.com/tropicsquare/libtropic-arduino/"
  },
  "frameworks": "arduino",
  "platforms": "*",
  "dependencies": {
    "libtropic": "https://github.com/tropicsquare/libtropic.git#ETR01SDK-298-Implement-Arduino-port"
  },
  "build": {
    "srcFilter": [
      "+<*>",
      "-<src/sdk>"
    ],
    "libArchive": false,
    "flags": [
       "TODO"
    ],
    "cmake": {
        "sourceDir": "."
    }
